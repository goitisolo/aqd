  <Concept rdf:about="1">
    <notation>1</notation>
    <prefLabel>Valid</prefLabel>
    <definition>Data considered valid by the data provider for its related reporting purposes.</definition>
    <inScheme rdf:resource=""/>
  </Concept>

  <Concept rdf:about="2">
    <notation>2</notation>
    <prefLabel>Valid, but below detection limit measurement value given</prefLabel>
    <definition>Data considered valid by the data provider for its related reporting purposes. However, the value has been measured below the detection limit and the value reported is the detection limit.</definition>
    <inScheme rdf:resource=""/>
  </Concept>

  <Concept rdf:about="3">
    <notation>3</notation>
    <prefLabel>Valid, but below detection limit and number replaced by 0.5*detection limit</prefLabel>
    <definition>Data considered valid by the data provider for its related reporting purposes. However, the value has been measured below the detection limit and the value reported is half of this detection limit.</definition>
    <inScheme rdf:resource=""/>
  </Concept>

  <Concept rdf:about="4">
    <notation>-99</notation>
    <prefLabel>Not valid due to station maintenance or calibration</prefLabel>
    <definition>Data is considered to be invalid due to the regular calibration or the normal maintenance of the instrumentation.</definition>
    <inScheme rdf:resource=""/>
  </Concept>

  <Concept rdf:about="5">
    <notation>-1</notation>
    <prefLabel>Not valid due to other reasons or missing</prefLabel>
    <definition>Data is considered to be invalid due to other circumstances or data is simply missing.</definition>
    <inScheme rdf:resource=""/>
  </Concept>

